# Ignore bundler config and other Ruby related files.
.bundle
.byebug_history

# Ignore all logfiles and tempfiles.
log/*
tmp/*
!log/.keep
!tmp/.keep

# Ignore uploaded files in development.
storage/*
!storage/.keep

# Ignore master key for decrypting credentials.
config/master.key

# Ignore front-end related files.
app/assets/builds/*
!app/assets/builds/.keep
node_modules/
public/assets/
yarn-error.log
yarn-debug.log*
.yarn-integrity

# Ignore secrets and other files.
.env*
!.env.example
docker-compose.override.yml

# Ignore asdf settings
.tool-versions

# Ignore YARD generated docs -- generate these on your own or in CI.
doc
doc/**/*
